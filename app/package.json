{
  "name": "dp",
  "version": "0.0.0",
  "description": "",
  "main": "dist/code",
  "types": "dist/types",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "build:no-types": "NO_TYPES=1 rollup --config rollup.config.js",
    "clean": "rimraf \"{dist,dist-app}/*\"",
    "lint": "eslint --ext js,ts .",
    "lint-fix": "eslint --fix --ext js,ts .",
    "playground": "ts-node --project tsconfig.start.json playground.ts",
    "playground:inspect": "TS_NODE_PROJECT=tsconfig.start.json node -r ts-node/register --debug playground.ts",
    "start": "node --trace-warnings -r source-map-support/register dist-app/index.js",
    "start-from-sources": "ts-node --project tsconfig.start.json src/index.ts",
    "start:inspect": "node --inspect-brk -r source-map-support/register dist/index.js",
    "style": "prettier --check .",
    "style-fix": "prettier --write .",
    "test": "run-s type-check style lint unit",
    "type-check": "tsc --noemit",
    "unit": "TS_NODE_PROJECT=tsconfig.start.json NODE_ENV=test mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thomaash/dp.git"
  },
  "author": "tom.vycital@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Thomaash/dp/issues"
  },
  "homepage": "https://github.com/Thomaash/dp#readme",
  "files": [
    "dist",
    "dist-app"
  ],
  "config": {
    "snap-shot-it": {
      "sortSnapshots": true,
      "useRelativePath": true
    }
  },
  "dependencies": {
    "agentkeepalive": "^4.1.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "jstat": "^1.9.4",
    "seedrandom": "^3.0.5",
    "source-map-support": "^0.5.19",
    "typedjson": "^1.5.2",
    "typescript-collections": "^1.3.3",
    "wait-port": "^0.2.9",
    "xml2js": "^0.4.23",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.2",
    "@types/mocha": "^8.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.1.1",
    "@types/seedrandom": "^2.4.28",
    "@types/sinon": "^9.0.6",
    "@types/sinon-chai": "^3.2.5",
    "@types/xml2js": "^0.4.5",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "builtin-modules": "^3.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "snap-shot-it": "^7.9.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
