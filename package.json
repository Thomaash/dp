{
  "name": "dp",
  "version": "0.0.0",
  "description": "",
  "main": "dist/code",
  "types": "dist/types",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "eslint --ext js,ts .",
    "playground": "ts-node --project tsconfig.start.json playground.ts",
    "playground:inspect": "TS_NODE_PROJECT=tsconfig.start.json node -r ts-node/register --inspect-brk playground.ts",
    "start": "node -r source-map-support/register dist/index.js",
    "start-from-sources": "ts-node --project tsconfig.start.json src/index.ts",
    "start:inspect": "node --inspect-brk -r source-map-support/register dist/index.js",
    "test": "run-s type-check lint unit",
    "type-check": "tsc --noemit",
    "unit": "TS_NODE_PROJECT=tsconfig.start.json NODE_ENV=test mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thomaash/dp.git"
  },
  "author": "tom.vycital@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Thomaash/dp/issues"
  },
  "homepage": "https://github.com/Thomaash/dp#readme",
  "files": ["dist"],
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "seedrandom": "^3.0.5",
    "source-map-support": "^0.5.16",
    "typescript-collections": "^1.3.3",
    "xml2js": "^0.4.23",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.5.5",
    "@types/prettier": "^1.19.0",
    "@types/seedrandom": "^2.4.28",
    "@types/sinon": "^7.5.2",
    "@types/sinon-chai": "^3.2.3",
    "@types/xml2js": "^0.4.5",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "builtin-modules": "^3.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rollup": "^2.0.6",
    "rollup-plugin-typescript2": "^0.26.0",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "wait-port": "^0.2.7"
  }
}
